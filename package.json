{
  "name": "snowpack-plugin-webpack",
  "version": "0.0.11",
  "description": "A Webpack 5 plugin for Snowpack",
  "main": "build/index.js",
  "typings": "build/main/index.d.ts",
  "repository": "https://github.com/branchard/snowpack-plugin-webpack",
  "license": "MIT",
  "keywords": [
    "snowpack",
    "plugin",
    "webpack",
    "webpack5",
    "webpack 5"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "lint": "eslint src --ext .ts",
    "reset": "git clean -dfx && git reset --hard && yarn"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.24.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "files": [
    "build",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "LICENSE",
    "README.md"
  ]
}
